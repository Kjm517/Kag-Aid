<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_doctor_name"
        android:layout_width="357dp"
        android:layout_height="36dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/place_autocomplete_prediction_primary_text_highlight"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="19dp" />

    <TextView
        android:id="@+id/textView_doctor_location"
        android:layout_width="343dp"
        android:layout_height="23dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView_doctor_name"
        android:layout_marginStart="10dp"
        android:text="TextView"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="52dp" />

    <TextView
        android:id="@+id/textView_doctor_contact"
        android:layout_width="343dp"
        android:layout_height="23dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView_doctor_location"
        android:layout_marginStart="10dp"
        android:text="TextView"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="75dp" />
</RelativeLayout>